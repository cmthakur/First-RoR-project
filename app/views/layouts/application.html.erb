<!DOCTYPE html>
<html>
<head>
  <title>Pms</title>
  <%= stylesheet_link_tag "style", "formtastic", "formtastic_changes" %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <header>
    <%= link_to "Managing Projects with PMS", projects_path %>
  </header>
  <hr/>
  <nav>
    <%= render "devise/shared/logged_in"%>
  </nav>
  <div id="notice">
    <%= notice %>
    <%= alert %>
  </div>

  <%= yield %>

</body>
</html>
